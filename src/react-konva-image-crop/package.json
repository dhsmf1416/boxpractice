{
  "name": "react-konva-image-crop",
  "description": "React Konva sandbox",
  "version": "0.0.9",
  "keywords": [],
  "dependencies": {
    "@reach/router": "^1.2.1",
    "babel-runtime": "6.26.0",
    "konva": "3.2.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-exif-orientation-img": "0.1.5",
    "react-konva": "16.8.4",
    "react-pinch-and-zoom": "1.1.3",
    "react-pinch-zoom-pan": "1.4.5",
    "react-prefixr": "0.1.0",
    "react-prismazoom": "1.1.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "use-image": "1.0.2"
  },
  "devDependencies": {
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
